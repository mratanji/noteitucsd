<body>

    <div class="jumbotron">
        <h1><center><a style="color: #FFFFFF" href="/">NOTE IT!</a></h1>
    </div>

    <div class="container">

    <form>
        <div class="form-group">
            <input type="email" class="form-control" name="real_name" id="real_name" placeholder="Name">
        </div>

        <div class="form-group">
            <input type="text" class="form-control" name="uname" id="uname" placeholder="Username">
        </div>

        <div class="form-group">
            <input type="password" class="form-control" name="pass" id="pass" placeholder="Password">
        </div>

        <a href="/account_done" class="btn btn-primary btn-lg btn-block active" role="button">Submit</a>
    </form>

    
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
        <script src="https://code.jquery.com/jquery.js"></script>
        <script src="/js/jquery-1.11.0.js"></script>
        <script src="/js/bootstrap.js"></script>
        <script src="/js/noteitucsd.js"></script>
</body>
</html>